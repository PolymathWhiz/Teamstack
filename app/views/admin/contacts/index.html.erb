    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <h2>
                  There are <%= pluralize(@count, "contact message") %> in total.
                </h2>
            </div>
            <!-- Basic Examples -->
            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="card">
                        <div class="header">
                            <h2>
                                CONTACT MESSAGES
                            </h2>
                            <ul class="header-dropdown m-r--5">
                                <li class="dropdown">
                                    <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                        <i class="material-icons">more_vert</i>
                                    </a>
                                    <ul class="dropdown-menu pull-right">
                                        <li><a href="javascript:void(0);">Action</a></li>
                                        <li><a href="javascript:void(0);">Another action</a></li>
                                        <li><a href="javascript:void(0);">Something else here</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                                    <thead>
                                        <tr>
                                            <th>UID</th>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Skill</th>
                                            <th>City</th>
                                            <th>State</th>
                                            <th>Country</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                  <% @contacts.each_with_index do |f, index|%>
                                    <% index += 1 %>
                                    <tbody>
                                        <tr>
                                            <td> <%= index %></td>
                                            <td><%= f.first_name %> <%= f.last_name %></td>
                                            <td><%= f.email %></td>
                                            <td><%= f.skill %></td>
                                            <td><%= f.city %></td>
                                            <td><%= f.state %></td>
                                            <td><%= f.country %></td>
                                            <% if current_user.admin? && current_user.id != f.id %>
                                              <td>
                                                <%= link_to 'Delete', f, class: "btn btn-danger waves-effect", method: :delete, data: { confirm: "Are you sure you want to delete this user ?" } %>
                                              </td>
                                              <% else %>
                                              <td><a href="#" class="btn btn-primary waves-effect">Your account</a></td>
                                            <% end %>
                                        </tr>
                                    </tbody>
                                  <% end %>
                                </table>
                              <%= paginate @contacts %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- #END# Basic Examples -->
        </div>
    </section>
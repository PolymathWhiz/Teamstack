                                        <li><a href="javascript:void(0);">Something else here</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="body">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                                    <thead>
                                        <tr>
                                            <th>UID</th>
                                            <th>Name</th>
                                            <th>Email</th>
                                            <th>Skill</th>
                                            <th>City</th>
                                            <th>State</th>
                                            <th>Country</th>
                                            <th>Edit</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                  <% @users.each do |f|%>
                                    <tbody>
                                        <tr>
                                            <td> <%= f.id %></td>
                                            <td><%= f.first_name %> <%= f.last_name %></td>
                                            <td><%= f.email %></td>
                                            <td><%= f.skill %></td>
                                            <td><%= f.city %></td>
                                            <td><%= f.state %></td>
                                            <td><%= f.country %></td>
                                            <td><button type="button" class="btn btn-warning waves-effect">Edit</button></td>
                                            <% if current_user.admin? %>
                                              <td>
                                                <%= link_to "Delete", f, class: "btn btn-danger waves-effect", method: :delete, data: { confirm: "Are you sure you want to delete this user ?" } %>
                                              </td>
                                            <% end %>
                                        </tr>
                                    </tbody>
                                  <% end %>
                                </table>
                              <%= paginate @users %>
